<h1>Log In</h1>

<%= form_for @login do |f| %>
    <ul>
        <li>
        <%= f.label :username %>
        <%= f.text_field :username %>
        </li>
        <li>
        <%= f.label :password%>
        <%= f.password_field :password %>
        </li>
    </ul>
    <%= f.submit :'Log In'%>

<% end %>
